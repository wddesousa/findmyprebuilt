{
  "name": "findmyprebuilt",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "test:int": "npm run docker:up && dotenv -e .env.test -- npx prisma db push && dotenv -e .env.test -- vitest --bail=1 -c ./vitest.config.integration.ts",
    "test:unit": "dotenv -e .env.test -- vitest --bail=1 -c ./vitest.config.unit.ts",
    "test:unit:ui": "dotenv -e .env.test -- vitest -c ./vitest.config.unit.ts --ui",
    "migrate:reset": "dotenv -e .env.development -- npx prisma migrate reset && dotenv -e .env.development -- prisma db seed",
    "seed:dev": "dotenv -e .env.development -- tsx prisma/seed.development.ts"
  },
  "prisma": {
    "seed": "dotenv -e .env.development -- tsx prisma/seed.development.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.0.1",
    "axios": "^1.7.9",
    "bullmq": "^5.41.4",
    "cheerio": "^1.0.0",
    "crypto": "^1.0.1",
    "ioredis": "^5.5.0",
    "next": "^15.2",
    "puppeteer": "^24.0.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "puppeteer-real-browser": "^1.3.20",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "slugify": "^1.6.6",
    "use-debounce": "^10.0.4",
    "xvfb": "^0.4.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/cheerio": "^0.22.35",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^8",
    "eslint-config-next": "15.0.3",
    "jsdom": "^25.0.1",
    "postcss": "^8",
    "prettier": "3.4.2",
    "prisma": "^6.0.1",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.19.2",
    "typescript": "^5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.9",
    "vitest-mock-extended": "^2.0.2"
  }
}
